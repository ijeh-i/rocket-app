{
	"name": "rocket-app",
	"version": "0.1.1",
	"private": true,
	"dependencies": {
		"@electron/remote": "^1.0.4",
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"axios": "^0.21.1",
		"concurrently": "^6.0.0",
		"cross-env": "^7.0.3",
		"dva": "^2.4.0",
		"dva-loading": "^2.0.5",
		"dva-model-extend": "^0.1.2",
		"electron-is-dev": "^2.0.0",
		"electron-updater": "^4.3.9",
		"moment": "^2.29.1",
		"path": "^0.12.7",
		"polished": "^4.1.1",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-s-alert": "^1.4.1",
		"react-scripts": "4.0.1",
		"styled-components": "^5.2.1",
		"wait-on": "^5.3.0",
		"web-vitals": "^0.2.4",
		"workbox-background-sync": "^5.1.3",
		"workbox-broadcast-update": "^5.1.3",
		"workbox-cacheable-response": "^5.1.3",
		"workbox-core": "^5.1.3",
		"workbox-expiration": "^5.1.3",
		"workbox-google-analytics": "^5.1.3",
		"workbox-navigation-preload": "^5.1.3",
		"workbox-precaching": "^5.1.3",
		"workbox-range-requests": "^5.1.3",
		"workbox-routing": "^5.1.3",
		"workbox-strategies": "^5.1.3",
		"workbox-streams": "^5.1.3"
	},
	"main": "public/main.js",
	"homepage": "./",
	"scripts": {
		"start": "PORT=8000 REACT_APP_ENVIRONMENT=development react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"electron:serve": "concurrently -k \" cross-env BROWSER=none npm start\" \"npm run-script electron:start\"",
		"electron:build-mac": "REACT_APP_ENVIRONMENT=staging REACT_APP_NATIVE=desktop npm build  && electron-builder -m -c.extraMetadata.main=build/main.js",
		"electron:build-win": "set \"REACT_APP_ENVIRONMENT=staging\" && set \"REACT_APP_NATIVE=desktop\" && npm build  && electron-builder --win -c.extraMetadata.main=build/main.js",
		"electron:start": "wait-on tcp:8000 && electron ."
	},
	"build": {
		"productName": "Rocket Shop",
		"extends": null,
		"appId": "com.example.electrong-cra",
		"publish": [],
		"files": [
			"dist/**/*",
			"build/**/*",
			"node_modules/**/*",
			"package.json"
		],
		"directories": {
			"buildResources": "assets"
		},
		"mac": {
			"icon": "public/logo192.png"
		},
		"win": {
			"icon": "public/logo192.png"
		}
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"electron": "^12.0.2",
		"electron-builder": "^22.10.5"
	}
}
